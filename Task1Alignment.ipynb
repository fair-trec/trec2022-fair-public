    "    tgt = mean_outer(avg_pages, raw_pages)\n",
    "\n",
    "    # average with background distributions\n",
    "    tgt = avg_with_bg(tgt)\n",
    "    \n",
    "    # and return the result\n",
    "    return tgt"
