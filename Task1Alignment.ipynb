    "def query_xalign(pages):\n",
    "    # compute targets to average\n",
    "    avg_pages = reduce(operator.mul, [d.align.loc[pages] for d in avg_dims])\n",
    "    raw_pages = reduce(operator.mul, [d.align.loc[pages] for d in raw_dims])\n",
    "\n",
